{
    "name": "global-x-landing",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --open --host",
        "build": "tsc && vite build",
        "preview": "vite preview --open --host",
        "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx}\"",
        "lint:stylelint": "stylelint --cache -q \"**/*.css\"",
        "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,jsx,json,html,css,cjs}\"",
        "lint:prettier:fix": "prettier --write \"**/*.{js,ts,tsx,jsx,json,html,css,cjs}\"",
        "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
        "prepare": "husky"
    },
    "dependencies": {
        "@radix-ui/react-avatar": "^1.0.4",
        "@radix-ui/react-slot": "^1.0.2",
        "@react-hookz/web": "^24.0.4",
        "@tanstack/react-query": "^5.40.0",
        "@tanstack/react-router": "^1.34.9",
        "clsx": "^2.1.1",
        "framer-motion": "^11.2.10",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@tanstack/eslint-plugin-query": "^5.35.6",
        "@tanstack/react-query-devtools": "^5.40.0",
        "@tanstack/router-devtools": "^1.34.9",
        "@tanstack/router-vite-plugin": "^1.34.8",
        "@types/node": "^20.14.0",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.11.0",
        "@typescript-eslint/parser": "^7.11.0",
        "@vitejs/plugin-react": "^4.3.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "husky": "^9.0.11",
        "prettier": "^3.3.0",
        "rollup-plugin-visualizer": "^5.12.0",
        "stylelint": "^16.6.1",
        "stylelint-config-standard": "^36.0.0",
        "typescript": "^5.2.2",
        "vite": "^5.2.0",
        "vite-plugin-eslint": "^1.8.1"
    }
}
